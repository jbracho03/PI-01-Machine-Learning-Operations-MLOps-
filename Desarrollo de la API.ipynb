   "source": [
    "def get_max_duration(year=None, platform=None, duration_type=None):\n",
    "  # Verifica si se ha especificado un año\n",
    "  if year is not None:\n",
    "    datos_loc = datos[datos['release_year'] == year]\n",
    "  else:\n",
    "    datos_loc = datos\n",
    "\n",
    "  # Verifica si se ha especificado una plataforma\n",
    "  if platform is not None:\n",
    "    datos_loc = datos_loc[datos_loc['id'].str.startswith(platform, na=False)]\n",
    "\n",
    "  # Verifica si se ha especificado un tipo de duración\n",
    "  if duration_type is not None:\n",
    "    datos_loc = datos_loc[datos_loc['duration_type'] == duration_type]\n",
    "\n",
    "  # Ordena la datos por duración y selecciona la película con mayor duración\n",
    "  datos_loc = datos_loc.sort_values(by='duration_int', ascending=False, na_position='last')\n",
    "  max_duration = datos_loc.iloc[0]\n",
    "\n",
    "  # Devuelve la película con mayor duración\n",
    "  return {\n",
    "    \"año\": str(max_duration['release_year']),\n",
    "    \"nombre\": max_duration['title'],\n",
    "    \"duracion\": str(max_duration['duration_int']),\n",
    "  }\n"
   ]
  },
